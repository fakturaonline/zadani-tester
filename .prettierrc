{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "printWidth": 120,
  "endOfLine": "auto",
  "plugins": [ "prettier-plugin-tailwindcss"],
  "importOrder": ["server-only", "^react$", "^styled-components", "^next", "^@mui", "^[a-zA-Z]", "^@/", "^\\."],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ]
}
